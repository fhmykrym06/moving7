  "cleanUrls": true,
  "trailingSlash": false,
  
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/companies",
      "dest": "/companies.html"
    },
    {
      "src": "/compare",
      "dest": "/compare.html"
    },
    {
